{
  "name": "startify5",
  "version": "1.0.0",
  "description": "Development environment for website creation with design and static coding.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev --port 2000",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview --port 8000",
    "astro": "astro",
    "lint": "npm run lint:style && npm run lint:es && npm run lint:html",
    "lint:style": "stylelint 'src/**/*.{css,scss,astro,vue}'",
    "lint:es": "eslint 'src/**/*.{astro,tsx,ts,vue}'",
    "lint:html": "markuplint 'src/**/*.{html,astro,tsx,vue}'",
    "format": "prettier --ignore-path .prettierignore --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DesignSupply/startify5.git"
  },
  "bugs": {
    "url": "https://github.com/DesignSupply/startify5/issues",
    "email": "info@designsuply-web.com"
  },
  "author": {
    "name": "Ogawa Shinya",
    "email": "info@designsuply-web.com",
    "url": "https://designsupply-web.com/"
  },
  "devDependencies": {
    "@markuplint/astro-parser": "^4.6.2",
    "@markuplint/jsx-parser": "^4.7.2",
    "@markuplint/react-spec": "^4.5.2",
    "@markuplint/vue-parser": "^4.6.4",
    "@markuplint/vue-spec": "^4.5.4",
    "@types/jquery": "^3.5.30",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "autoprefixer": "^10.4.19",
    "css-declaration-sorter": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.2.0",
    "markuplint": "^4.8.1",
    "postcss-html": "^1.7.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "stylelint": "^16.5.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.3.0",
    "typescript-eslint": "^7.14.1"
