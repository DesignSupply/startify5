    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.6.3",
    "jquery": "^3.7.1",
    "pinia": "^2.1.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.77.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vue": "^3.4.30"
  },
  "browserslist": [
    "last 2 versions, > 1%"
  ]
